<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Praktikum_4_1_home">
    
    <TextView
        android:id="@+id/tv4_pra_4_1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="INI MAINACTIVITY KE 2"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/tv5_pra_4_1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:text="Shared Preferences Yang Tersimpan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv4_pra_4_1_home"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/tv6_pra_4_1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="50dp"
        android:text="Nama"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv5_pra_4_1_home"/>

    <TextView
        android:id="@+id/tv7_pra_4_1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="11dp"
        android:text="Jenis Kelamin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv6_pra_4_1_home"/>

    <TextView
        android:id="@+id/tv8_pra_4_1_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="9dp"
        android:text="Usia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv7_pra_4_1_home"/>

    <TextView
        android:id="@+id/tvNama_pra_4_1_home"
        android:layout_width="134dp"
        android:layout_height="20dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="50dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv6_pra_4_1_home"
        app:layout_constraintTop_toBottomOf="@+id/tv5_pra_4_1_home" />

    <TextView
        android:id="@+id/tvJenisKel_pra_4_1_home"
        android:layout_width="134dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv7_pra_4_1_home"
        app:layout_constraintTop_toBottomOf="@+id/tvNama_pra_4_1_home" />

    <TextView
        android:id="@+id/tvUsia_pra_4_1_home"
        android:layout_width="134dp"
        android:layout_height="20dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv8_pra_4_1_home"
        app:layout_constraintTop_toBottomOf="@+id/tvJenisKel_pra_4_1_home" />

</androidx.constraintlayout.widget.ConstraintLayout>